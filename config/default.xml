<entry key="SPLIT_ADD_ALL">false</entry>
<entry key="SPLIT_SOLVE_CSP">true</entry>
<entry key="SPLIT_ADD_ONLY_CSP">false</entry>    
<entry key="SPLIT_PULSE_NUM_LABELS">3</entry> 

<entry key="MSH_MAX_POOL_SIZE">50000</entry> 
<entry key="MSH_NUM_ITERATIONS">1000</entry>
<entry key="MSH_SAMPLING_TIME_LIMIT">3600</entry>
<entry key="MSH_USE_HEURISTICS">0</entry>
<entry key="MSH_RANDOM_FACTOR_HIGH">10</entry>
<entry key="MSH_RANDOM_FACTOR_LOW">3</entry>
<entry key="MSH_ASSEMBLY_TIME_LIMIT">60</entry>

</properties>
