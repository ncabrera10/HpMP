<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<entry key="INSTANCE_FOLDER">./instances/</entry>
<entry key="RESULT_FOLDER">./results/</entry> 
<entry key="SOLUTIONS_FOLDER">./solutions/</entry> 
 
<entry key="PRINT_IN_CONSOLE">false</entry>

<entry key="PRECISION">4</entry> 
<entry key="SEED">1</entry> 
<entry key="THREADS">4</entry> 
<entry key="TIME_OUT_POOL_MS">100</entry> 

